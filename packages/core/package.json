    "@lingui/cli": "^3.16.0",
    "@lingui/core": "^3.16.0",
    "@lingui/macro": "^3.16.0",
    "@lingui/react": "^3.16.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
