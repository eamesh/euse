{
  "name": "@euse/core",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "dev": "pnpm build:watch:types && pnpm build:watch",
    "build": "rimraf dist && pnpm build:types && pnpm locale && pnpm build:js",
    "build:js": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:types": "tsc --emitDeclarationOnly",
    "build:watch:types": "tsc --watch --emitDeclarationOnly",
    "locale": "pnpm locale:extract && pnpm locale:compile",
    "locale:clean": "lingui extract --clean",
    "locale:extract": "lingui extract",
    "locale:compile": "lingui compile --typescript",
    "check:types": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@lingui/react": "^3.16.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@mui/system": "^5.11.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@euse/api-react": "workspace:^0.1.0",
    "@lingui/cli": "^3.16.0",
    "@lingui/core": "^3.16.0",
    "@lingui/macro": "^3.16.0",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-plugin-direct-import": "^1.0.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "query-string": "^8.1.0",
    "react-use": "^17.4.0",
    "react-use-timeout": "^1.0.0",
    "rimraf": "^4.0.5",
    "rollup": "^3.10.0",
    "rollup-plugin-node-externals": "^5.1.0",
