    "rollup": "^3.9.1",
    "rollup-plugin-import-css": "^3.1.0",
    "rollup-plugin-node-externals": "^5.0.3",
    "stacktrace-js": "^2.0.2",
    "styled-components": "^5.3.6",
    "tss-react": "^4.5.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.4"
  },
