{
  "presets": [
    "@babel/env",
    "@babel/typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "macros",
    "@babel/plugin-transform-runtime",
    ["babel-plugin-direct-import", { "modules": ["@mui/material", "@mui/icons-material"] }]
  ]
}
