{
  "name": "@euse/api-react",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=16.15.0"
  },
  "main": "dist/index.js",
  "module": "dist/esm.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "dev": "pnpm build:watch:types && pnpm build:watch",
    "build": "pnpm build:types && pnpm locale && pnpm build:js",
    "build:js": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:types": "tsc --emitDeclarationOnly",
    "build:watch:types": "tsc --watch --emitDeclarationOnly",
    "locale:extract": "lingui extract",
    "locale:clean": "lingui extract --clean",
    "locale:compile": "lingui compile",
    "locale": "lingui extract && lingui compile",
    "check:types": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-transform-imports": "^2.0.0",
    "react-is": "^18.2.0",
    "rollup": "^3.9.1",
    "rollup-plugin-node-externals": "^5.0.3",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  }
}
